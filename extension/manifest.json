
{
  "manifest_version": 3,
  "name": "Phishing Detector",
  "description": "A Chrome extension to detect phishing URLs using a Flask API.",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons.png",
      "48": "icons.png",
      "128": "icons.png"
    }
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "http://localhost:5001/*"
  ]
}